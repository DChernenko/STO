@model  STO.Domain.Entities.TotalPrice

@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Details</h2>
<div class="row">
    <div class="col-6 col-md-4">
        Номер автомобиля:
    </div>
    <div class="col-6 col-md-4">@Model.Car.Number</div>
</div>

<div class="row">
    <div class="col-6 col-md-4">
        Дата:
    </div>
    <div class="col-6 col-md-4">@Model.Date</div>
</div>

<div class="row">
    <div class="col-6 col-md-4">
        Общая стоймось:
    </div>
    <div class="col-6 col-md-4">@Model.Total</div>
</div>





